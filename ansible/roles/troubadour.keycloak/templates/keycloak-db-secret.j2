apiVersion: v1
kind: Secret
type: Opaque     
metadata:         
  name: keycloak-db-secret 
  namespace: keycloak
stringData:         
  POSTGRES_USERNAME: "{{ keycloak.pg.user }}"
  POSTGRES_PASSWORD: "{{ keycloak.pg.pass }}"
  POSTGRES_DATABASE: "{{ keycloak.pg.db }}"
  POSTGRES_HOST: "keycloak-postgresql"
  POSTGRES_EXTERNAL_ADDRESS: "troubadour-postgresql.postgresql.svc.cluster.local"
  POSTGRES_EXTERNAL_PORT: "5432"     
  POSTGRES_SUPERUSER: "true"   
